<Window x:Class="AnimeProgram.filterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnimeProgram"
        mc:Ignorable="d"
        Title="filterWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>

        </Grid.ColumnDefinitions>
        <ComboBox x:Name="cb_img" Width="200" Height="50" SelectionChanged="cb_img_SelectionChanged"/>
        <ComboBox x:Name="cb_gif" Width="200" Height="50" Grid.Column="1" SelectionChanged="cb_gif_SelectionChanged"/>
    </Grid>
</Window>
